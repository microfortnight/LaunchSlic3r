				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run JavaScript.action</string>
				<key>ActionName</key>
				<string>Run JavaScript</string>
				<key>ActionNameComment</key>
				<string>Filter apps starting with "Slic3r", choose Slic3r version from list</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>function run(input, parameters) {	var app = Application.currentApplication()	app.includeStandardAdditions = true     var slic3rs = []	    for (var i = 0; i &lt; input.length; i++) {		var appPath = input[i].toString()		var appName = appPath.split('\\').pop().split('/').pop();		if (appName.startsWith("Slic3r")) {			slic3rs.push(appPath);		}    }		var slic3r = app.chooseFromList(slic3rs, {			withPrompt: 'Choose Slic3r version:',			multipleSelectionsAllowed: false	});	var s = slic3r.toString();	var appVersion = s.substring(s.indexOf("-")+1, s.lastIndexOf("."));	//app.displayDialog(slic3r);	return [slic3r, appVersion];}</string>
				</dict>
