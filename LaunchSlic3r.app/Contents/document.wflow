				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run AppleScript.action</string>
				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionNameComment</key>
				<string>Launch Slic3r</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}		-- Name of the folder containing the Slic3r profiles	set Slic3rProfilesFolderName to "Slic3rProfiles"		-- Location of the folder containing the Slic3r profiles	-- ownCloud, Dropbox, ~/Library/Application Support, ...	--set Slic3rProfilesParent to ((path to home folder as string) &amp; "ownCloud:")	--set Slic3rProfilesParent to ((path to home folder as string) &amp; "Dropbox:")	set Slic3rProfilesParent to ((path to library folder from user domain as text) &amp; "Application Support:")		--Create folder if it doesn't exist	set Slic3rProfilesRoot to Slic3rProfilesParent &amp; Slic3rProfilesFolderName	tell application "Finder"		if (not (exists (folder Slic3rProfilesRoot))) then			make new folder at Slic3rProfilesParent with properties {name:Slic3rProfilesFolderName}		end if	end tell			set applicationPath to item 1 of input	set applicationVersion to item 2 of input		--set libraryPath to "/Users/till/Library/Application Support/Slic0r/" &amp; item 2 of input &amp; "/"	--set libraryPathQuoted to quoted form of libraryPath	--set settingsPath to "~/Documents/Slic3rPE-" &amp; item 2 of input		set settingsPath to Slic3rProfilesRoot &amp; ":" &amp; applicationVersion	--set settingsPathQuoted to quoted form of settingsPath	set settingsPathQuoted to quoted form of the POSIX path of settingsPath	set shellScript to "open -a " &amp; applicationPath &amp; " --args --datadir " &amp; settingsPathQuoted	do shell script shellScript		return settingsPathQuotedend run</string>
				</dict>
